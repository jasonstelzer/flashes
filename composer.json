{
    "name": "jasonstelzer/flashes",
    "description": "Laravel 5 flash messages",
    "keywords": [
        "flashes"
    ],
    "homepage": "https://github.com/jasonstelzer/flashes",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Stelzer",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Jasonstelzer\\Flashes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jasonstelzer\\Flashes\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
